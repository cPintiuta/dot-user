{
    "name": "dotkernel/dot-user",
    "type": "library",
    "description": "DotKernel configurable user component providing common user functions and extending the authentication flow",
    "license": "MIT",
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "mezzio/mezzio-template": "^2.0",
        "laminas/laminas-diactoros": "^1.7",
        "laminas/laminas-servicemanager": "^3.3",
        "laminas/laminas-db": "^2.8",
        "laminas/laminas-math": "^3.0",
        "laminas/laminas-crypt": "^3.1",
        "dotkernel/dot-helpers": "^2.0",
        "dotkernel/dot-event": "^2.0",
        "dotkernel/dot-mapper": "^2.0",
        "dotkernel/dot-filter": "^2.0",
        "dotkernel/dot-validator": "^2.0",
        "dotkernel/dot-inputfilter": "^2.0",
        "dotkernel/dot-hydrator": "^2.0",
        "dotkernel/dot-form": "^2.0",
        "dotkernel/dot-flashmessenger": "^2.0",
        "dotkernel/dot-controller": "^2.0",
        "dotkernel/dot-controller-plugin-flashmessenger": "^2.0",
        "dotkernel/dot-controller-plugin-authentication": "^2.0",
        "dotkernel/dot-controller-plugin-authorization": "^2.0",
        "dotkernel/dot-controller-plugin-forms": "^2.0",
        "dotkernel/dot-authentication": "^2.0",
        "dotkernel/dot-authentication-web": "^2.0",
        "dotkernel/dot-authorization": "^2.0",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3",
        "laminas/laminas-captcha": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Dot\\User\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\User\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "2.1-dev"
        }
    }
}
