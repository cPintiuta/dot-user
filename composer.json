{
  "name": "dotkernel/dot-user",
  "type": "library",
  "description": "DotKernel configurable user component providing common user functions and extending the authentication flow",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/http-message": "^1.0",
    "zendframework/zend-expressive-template": "^1.0",
    "zendframework/zend-diactoros": "^1.3",
    "zendframework/zend-servicemanager": "^3.3.0",
    "zendframework/zend-db": "^2.8",
    "zendframework/zend-math": "^3.0",
    "zendframework/zend-crypt": "^3.1",

    "dotkernel/dot-helpers": "^0.2",
    "dotkernel/dot-event": "^0.2",
    "dotkernel/dot-mapper": "^0.4",
    "dotkernel/dot-filter": "^1.1",
    "dotkernel/dot-validator": "^1.2",
    "dotkernel/dot-inputfilter": "^1.1",
    "dotkernel/dot-hydrator": "^1.1",
    "dotkernel/dot-form": "^1.1",
    "dotkernel/dot-flashmessenger": "^0.2",
    "dotkernel/dot-controller": "^0.2",
    "dotkernel/dot-controller-plugin-flashmessenger": "^0.2",
    "dotkernel/dot-controller-plugin-authentication": "^0.2",
    "dotkernel/dot-controller-plugin-authorization": "^0.2",
    "dotkernel/dot-controller-plugin-forms": "^0.2",
    "dotkernel/dot-authentication": "^0.1",
    "dotkernel/dot-authentication-web": "^0.2",
    "dotkernel/dot-authorization": "^0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",
    "zendframework/zend-captcha": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "Dot\\User\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\User\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3-dev",
      "dev-develop": "0.4-dev"
    }
  }
}
